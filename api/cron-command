0 1 * * * . /root/project_env.sh && /usr/local/bin/python /usr/src/api/manage.py run_client > /proc/1/fd/1 2>/proc/1/fd/2
0 1 * * * . /root/project_env.sh && /usr/local/bin/python /usr/src/api/manage.py load_default_projects > /proc/1/fd/1 2>/proc/1/fd/2
0 0 28 * * . /root/project_env.sh && /usr/local/bin/python /usr/src/api/manage.py 28th_date_update > /proc/1/fd/1 2>/proc/1/fd/2
0 0 1-27,29-31 * 2 . /root/project_env.sh && /usr/local/bin/python /usr/src/api/manage.py tuesdays_date_update > /proc/1/fd/1 2>/proc/1/fd/2
*/1 * * * * . /root/project_env.sh && printenv > /root/res.txt
